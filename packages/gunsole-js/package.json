{
  "name": "gunsole-js",
  "version": "0.1.0",
  "description": "Gunsole JavaScript/TypeScript SDK for browser and Node.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "LICENSE", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "prepublishOnly": "pnpm build && pnpm typecheck"
  },
  "keywords": ["gunsole", "logging", "analytics", "developer-tools"],
  "publishConfig": {
    "access": "public"
  },
  "author": "push1kb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/push1kb/gunsole-js.git",
    "directory": "packages/gunsole-js"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "peerDependencies": {
    "typescript": "^5.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
